CREATE TABLE batchEvents (
  batchEventId INT NOT NULL AUTO_INCREMENT,
  batchId INT NOT NULL,
  event ENUM('refrigerated', 'frozen', 'thawed', 'consumed', 'shared', 'discarded') NOT NULL,
  eventDate DATE NOT NULL,
  notes TEXT,
  PRIMARY KEY (batchEventId),
  FOREIGN KEY (batchId) REFERENCES batches(batchId)
);
